<launch>
  <group ns="market">
    <param name="web3_http_provider" value="http://localhost:8545" />

    <node pkg="robonomics_control" type="distribution_node" name="distribution" output="screen">
      <param name="investors_contract_abi" textfile="$(find robonomics_control)/abi/investors.json" />
      <param name="investors_contract_address" value="0x7Dc07BE45D6547670e186568deB91a1A4E2E6e53" />
      <param name="supported_models" textfile="$(find robonomics_control)/models/simple.json" />
    </node>
  </group>
</launch>
